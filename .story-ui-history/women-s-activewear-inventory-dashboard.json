      "prompt": "Generate a product inventory dashboard for a women's activewear clothing line.",
      "code": "import React from 'react';\nimport type { Meta, StoryObj } from '@storybook/react';\nimport {\n  AppShell,\n  AppShellHeader,\n  AppShellNavbar,\n  AppShellMain,\n  Group,\n  Stack,\n  Text,\n  Title,\n  Button,\n  TextInput,\n  Select,\n  Table,\n  TableThead,\n  TableTbody,\n  TableTr,\n  TableTh,\n  TableTd,\n  TableScrollContainer,\n  Badge,\n  Avatar,\n  Paper,\n  SimpleGrid,\n  Progress,\n  ActionIcon,\n  Card,\n  Image,\n  RingProgress,\n  Center,\n  ThemeIcon,\n  Divider,\n  NavLink,\n  Burger,\n  ScrollArea\n} from '@mantine/core';\n\nconst meta: Meta = {\n  title: 'Generated/Women\\'s Activewear Inventory Dashboard',\n  parameters: {\n    layout: 'fullscreen',\n  },\n};\n\nexport default meta;\ntype Story = StoryObj;\n\nconst inventoryData = [\n  {\n    id: '1',\n    name: 'Seamless Sculpt Leggings',\n    sku: 'LG-001-BLK',\n    category: 'Leggings',\n    price: 68.00,\n    stock: 85,\n    capacity: 100,\n    status: 'In Stock',\n    image: 'https://picsum.photos/100/100?random=1'\n  },\n  {\n    id: '2',\n    name: 'Aero High-Impact Bra',\n    sku: 'BR-042-PNK',\n    category: 'Sports Bras',\n    price: 45.00,\n    stock: 12,\n    capacity: 80,\n    status: 'Low Stock',\n    image: 'https://picsum.photos/100/100?random=2'\n  },\n  {\n    id: '3',\n    name: 'Core Runner Jacket',\n    sku: 'JK-103-TEL',\n    category: 'Outerwear',\n    price: 92.00,\n    stock: 40,\n    capacity: 60,\n    status: 'In Stock',\n    image: 'https://picsum.photos/100/100?random=3'\n  },\n  {\n    id: '4',\n    name: 'Yoga Flow Tank',\n    sku: 'TP-205-WHT',\n    category: 'Tops',\n    price: 38.00,\n    stock: 0,\n    capacity: 50,\n    status: 'Out of Stock',\n    image: 'https://picsum.photos/100/100?random=4'\n  },\n  {\n    id: '5',\n    name: 'Recover Lounge Pant',\n    sku: 'LG-009-GRY',\n    category: 'Bottoms',\n    price: 55.00,\n    stock: 120,\n    capacity: 150,\n    status: 'In Stock',\n    image: 'https://picsum.photos/100/100?random=5'\n  },\n];\n\nexport const WomensActivewearDashboard: Story = {\n  render: () => (\n    <AppShell\n      header={{ height: 60 }}\n      navbar={{ width: 250, breakpoint: 'sm' }}\n      padding=\"md\"\n    >\n      <AppShellHeader>\n        <Group h=\"100%\" px=\"md\" justify=\"space-between\">\n          <Group>\n            <Burger opened={false} hiddenFrom=\"sm\" size=\"sm\" />\n            <ThemeIcon size=\"lg\" variant=\"gradient\" gradient={{ from: 'pink', to: 'orange' }} radius=\"xl\">\n              <Text fw={700} size=\"sm\">L</Text>\n            </ThemeIcon>\n            <Text fw={700} size=\"lg\">LUNA ATHLETICS</Text>\n          </Group>\n          <Group>\n            <TextInput placeholder=\"Search...\" style={{ width: 200 }} />\n            <Divider orientation=\"vertical\" />\n            <Group gap=\"xs\">\n              <Avatar src=\"https://picsum.photos/200/200?random=99\" radius=\"xl\" />\n              <div style={{ lineHeight: 1 }}>\n                <Text size=\"sm\" fw={500}>Sarah Jensen</Text>\n                <Text size=\"xs\" c=\"dimmed\">Store Manager</Text>\n              </div>\n            </Group>\n          </Group>\n        </Group>\n      </AppShellHeader>\n\n      <AppShellNavbar p=\"md\">\n        <Stack gap=\"xs\">\n          <Text size=\"xs\" fw={500} c=\"dimmed\" mb={4}>MAIN MENU</Text>\n          <NavLink label=\"Dashboard\" active variant=\"light\" color=\"pink\" />\n          <NavLink label=\"Inventory\" variant=\"subtle\" />\n          <NavLink label=\"Orders\" variant=\"subtle\" description=\"5 new orders\" />\n          <NavLink label=\"Customers\" variant=\"subtle\" />\n          <NavLink label=\"Analytics\" variant=\"subtle\" />\n          \n          <Divider my=\"sm\" />\n          \n          <Text size=\"xs\" fw={500} c=\"dimmed\" mb={4}>SETTINGS</Text>\n          <NavLink label=\"Store Settings\" variant=\"subtle\" />\n          <NavLink label=\"Team Members\" variant=\"subtle\" />\n          <NavLink label=\"Billing\" variant=\"subtle\" />\n        </Stack>\n      </AppShellNavbar>\n\n      <AppShellMain style={{ backgroundColor: '#f8f9fa' }}>\n        <Stack gap=\"lg\">\n          \n          <Group justify=\"space-between\" align=\"center\">\n            <div>\n              <Title order={2}>Inventory Overview</Title>\n              <Text c=\"dimmed\">Manage your activewear catalog and stock levels</Text>\n            </div>\n            <Button variant=\"filled\" color=\"dark\">Add New Product</Button>\n          </Group>\n\n          <SimpleGrid cols={3} spacing=\"md\">\n            <Paper p=\"md\" radius=\"md\" withBorder>\n              <Group justify=\"space-between\">\n                <div>\n                  <Text c=\"dimmed\" tt=\"uppercase\" fw={700} size=\"xs\">Total Products</Text>\n                  <Text fw={700} size=\"xl\">1,248</Text>\n                </div>\n                <ThemeIcon variant=\"light\" color=\"blue\" size=\"xl\" radius=\"md\">\n                  <Text size=\"xl\" fw={700}>P</Text>\n                </ThemeIcon>\n              </Group>\n              <Text c=\"teal\" size=\"sm\" fw={500} mt=\"md\">\n                +12% <Text span c=\"dimmed\" fw={400}>from last month</Text>\n              </Text>\n            </Paper>\n\n            <Paper p=\"md\" radius=\"md\" withBorder>\n              <Group justify=\"space-between\">\n                <div>\n                  <Text c=\"dimmed\" tt=\"uppercase\" fw={700} size=\"xs\">Low Stock Alerts</Text>\n                  <Text fw={700} size=\"xl\">12</Text>\n                </div>\n                <ThemeIcon variant=\"light\" color=\"red\" size=\"xl\" radius=\"md\">\n                  <Text size=\"xl\" fw={700}>!</Text>\n                </ThemeIcon>\n              </Group>\n              <Text c=\"red\" size=\"sm\" fw={500} mt=\"md\">\n                3 items <Text span c=\"dimmed\" fw={400}>out of stock</Text>\n              </Text>\n            </Paper>\n\n            <Paper p=\"md\" radius=\"md\" withBorder>\n              <Group>\n                <RingProgress\n                  size={80}\n                  roundCaps\n                  thickness={8}\n                  sections={[{ value: 65, color: 'pink' }]}\n                  label={\n                    <Center>\n                      <Text fw={700} size=\"xs\">65%</Text>\n                    </Center>\n                  }\n                />\n                <div>\n                  <Text c=\"dimmed\" tt=\"uppercase\" fw={700} size=\"xs\">Sell-through Rate</Text>\n                  <Text fw={700} size=\"xl\">Top: Leggings</Text>\n                </div>\n              </Group>\n            </Paper>\n          </SimpleGrid>\n\n          <Paper withBorder radius=\"md\" p=\"md\">\n            <Stack gap=\"md\">\n              <Group justify=\"space-between\">\n                <Group>\n                  <TextInput placeholder=\"Search products...\" style={{ width: 300 }} />\n                  <Select\n                    placeholder=\"Category\"\n                    data={['All', 'Leggings', 'Sports Bras', 'Tops', 'Outerwear']}\n                    defaultValue=\"All\"\n                    style={{ width: 150 }}\n                  />\n                  <Select\n                    placeholder=\"Stock Status\"\n                    data={['All', 'In Stock', 'Low Stock', 'Out of Stock']}\n                    defaultValue=\"All\"\n                    style={{ width: 150 }}\n                  />\n                </Group>\n                <Group>\n                  <Button variant=\"default\">Export</Button>\n                </Group>\n              </Group>\n\n              <TableScrollContainer minWidth={800}>\n                <Table verticalSpacing=\"sm\">\n                  <TableThead>\n                    <TableTr>\n                      <TableTh>Product</TableTh>\n                      <TableTh>Category</TableTh>\n                      <TableTh>Price</TableTh>\n                      <TableTh>Stock Level</TableTh>\n                      <TableTh>Status</TableTh>\n                      <TableTh />\n                    </TableTr>\n                  </TableThead>\n                  <TableTbody>\n                    {inventoryData.map((item) => (\n                      <TableTr key={item.id}>\n                        <TableTd>\n                          <Group gap=\"sm\">\n                            <Image\n                              src={item.image}\n                              w={40}\n                              h={40}\n                              radius=\"sm\"\n                              alt={item.name}\n                            />\n                            <div>\n                              <Text size=\"sm\" fw={500}>{item.name}</Text>\n                              <Text size=\"xs\" c=\"dimmed\">{item.sku}</Text>\n                            </div>\n                          </Group>\n                        </TableTd>\n                        <TableTd>\n                          <Badge variant=\"light\" color=\"gray\" radius=\"sm\">{item.category}</Badge>\n                        </TableTd>\n                        <TableTd>\n                          <Text size=\"sm\" fw={500}>${item.price.toFixed(2)}</Text>\n                        </TableTd>\n                        <TableTd width={200}>\n                          <Stack gap={4}>\n                            <Group justify=\"space-between\">\n                              <Text size=\"xs\" c=\"dimmed\">{item.stock} / {item.capacity}</Text>\n                              <Text size=\"xs\" fw={500}>{Math.round((item.stock / item.capacity) * 100)}%</Text>\n                            </Group>\n                            <Progress \n                              value={(item.stock / item.capacity) * 100} \n                              color={item.stock === 0 ? 'gray' : item.stock < 20 ? 'red' : 'teal'} \n                              size=\"sm\" \n                            />\n                          </Stack>\n                        </TableTd>\n                        <TableTd>\n                          <Badge \n                            color={item.status === 'In Stock' ? 'teal' : item.status === 'Low Stock' ? 'orange' : 'gray'}\n                            variant=\"dot\"\n                          >\n                            {item.status}\n                          </Badge>\n                        </TableTd>\n                        <TableTd>\n                          <Group gap=\"xs\" justify=\"flex-end\">\n                            <ActionIcon variant=\"subtle\" color=\"gray\">\n                              <Text size=\"sm\">✎</Text>\n                            </ActionIcon>\n                            <ActionIcon variant=\"subtle\" color=\"red\">\n                              <Text size=\"sm\">×</Text>\n                            </ActionIcon>\n                          </Group>\n                        </TableTd>\n                      </TableTr>\n                    ))}\n                  </TableTbody>\n                </Table>\n              </TableScrollContainer>\n            </Stack>\n          </Paper>\n        </Stack>\n      </AppShellMain>\n    </AppShell>\n  ),\n};",
      "fileName": "women-s-activewear-inventory-dashboard-2c7c6e52.stories.tsx"
    }
