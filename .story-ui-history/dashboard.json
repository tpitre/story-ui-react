      "code": "import React from 'react';\nimport type { Meta, StoryObj } from '@storybook/react';\nimport {\n  AppShell,\n  AppShellNavbar,\n  AppShellHeader,\n  AppShellMain,\n  Group,\n  Stack,\n  ActionIcon,\n  Text,\n  Title,\n  Button,\n  Avatar,\n  Paper,\n  SimpleGrid,\n  Grid,\n  GridCol,\n  Badge,\n  Table,\n  TableThead,\n  TableTbody,\n  TableTr,\n  TableTh,\n  TableTd,\n  Progress,\n  AvatarGroup,\n  Image,\n  Box,\n  Flex\n} from '@mantine/core';\n\nconst meta: Meta<typeof AppShell> = {\n  title: 'Generated/Dashboard',\n  id: 'dashboard-0bdf6804',\n  component: AppShell,\n  parameters: {\n    layout: 'fullscreen',\n  },\n  tags: ['autodocs'],\n};\n\nexport default meta;\ntype Story = StoryObj<typeof meta>;\n\n// Mock Data for Weather Cards\nconst weatherData = [\n  {\n    title: 'Calm Weather',\n    desc: 'The turbine may not spin at all or only spin very slowly',\n    rotation: '0-5 RPM',\n    wind: '0-3 m/s',\n    icon: '‚òÄÔ∏è',\n  },\n  {\n    title: 'Rainy Weather',\n    desc: 'The rotating speed control system does not exceed the safe limit.',\n    rotation: '20-25 RPM',\n    wind: '12-15 m/s',\n    icon: 'üåßÔ∏è',\n  },\n  {\n    title: 'Windy Weather',\n    desc: 'The rotating turbine varies proportionally with the wind speed',\n    rotation: '10-20 RPM',\n    wind: '4-12 m/s',\n    icon: 'üå•Ô∏è',\n  },\n  {\n    title: 'Stormy Weather',\n    desc: 'The turbine stops spinning or uses the brake to stop the rotation.',\n    rotation: '0 RPM',\n    wind: '15-25+ m/s',\n    icon: '‚ö°',\n  },\n];\n\n// Mock Data for Table\nconst tableData = [\n  {\n    name: 'Three-bladed HAWT',\n    power: '1 kW - 10 Mw',\n    rotation: '10-20 RPM',\n    wind: '3-25 m/s',\n    status: 'Active',\n    color: 'green',\n  },\n  {\n    name: 'Multi-Blade HAWT',\n    power: '500 W - 100 kW',\n    rotation: '20-60 RPM',\n    wind: '3-25 m/s',\n    status: 'Active',\n    color: 'green',\n  },\n  {\n    name: 'Darrieus VAWT',\n    power: '1 kW - several MW',\n    rotation: '10-200 RPM',\n    wind: '3-25 m/s',\n    status: 'Pending',\n    color: 'yellow',\n  },\n  {\n    name: 'Savonius VAWT',\n    power: '1 - 50 kW',\n    rotation: '10-50 RPM',\n    wind: '2-14 m/s',\n    status: 'Non-Active',\n    color: 'red',\n  },\n  {\n    name: 'Helical VAWT',\n    power: '1 - 10 kW',\n    rotation: '10-150 RPM',\n    wind: '3-20 m/s',\n    status: 'Active',\n    color: 'green',\n  },\n];\n\nexport const DashboardOverview: Story = {\n  render: () => (\n    <AppShell\n      padding=\"md\"\n      navbar={{ width: 80, breakpoint: 'sm' }}\n      header={{ height: 80 }}\n      style={{ backgroundColor: '#f8f9fa' }}\n    >\n      {/* Header Section */}\n      <AppShellHeader style={{ borderBottom: 'none', backgroundColor: 'transparent' }}>\n        <Group h=\"100%\" px=\"md\" justify=\"space-between\" align=\"center\">\n          <Title order={2}>Overview</Title>\n          <Group>\n            <ActionIcon variant=\"default\" size=\"lg\" radius=\"xl\" aria-label=\"Search\">\n              üîç\n            </ActionIcon>\n            <ActionIcon variant=\"default\" size=\"lg\" radius=\"xl\" aria-label=\"Notifications\">\n              üîî\n            </ActionIcon>\n            <Button\n              radius=\"xl\"\n              variant=\"filled\"\n              color=\"indigo\"\n              styles={{ root: { backgroundColor: '#5c5fec' } }}\n            >\n              + Add a new note\n            </Button>\n            <Avatar src=\"https://picsum.photos/id/64/200/200\" radius=\"xl\" alt=\"User Avatar\" />\n          </Group>\n        </Group>\n      </AppShellHeader>\n\n      {/* Navbar / Sidebar Section */}\n      <AppShellNavbar p=\"md\" style={{ backgroundColor: '#fff', borderRight: 'none' }}>\n        <Stack align=\"center\" gap=\"xl\">\n          <Box mb=\"xl\">\n            <Text c=\"blue\" size=\"xl\" fw={900} style={{ fontSize: '2rem' }}>\n              ‚ö°\n            </Text>\n          </Box>\n          <Stack gap=\"lg\">\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Home\">\n              ü™ü\n            </ActionIcon>\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Energy\">\n              ‚ö°\n            </ActionIcon>\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Network\">\n              üåê\n            </ActionIcon>\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Analytics\">\n              üìä\n            </ActionIcon>\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Calendar\">\n              üìÖ\n            </ActionIcon>\n          </Stack>\n          <Box style={{ flexGrow: 1 }} />\n          <Stack gap=\"lg\">\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Alerts\">\n              ‚ö†Ô∏è\n            </ActionIcon>\n            <ActionIcon variant=\"subtle\" color=\"gray\" size=\"xl\" aria-label=\"Settings\">\n              ‚öôÔ∏è\n            </ActionIcon>\n          </Stack>\n        </Stack>\n      </AppShellNavbar>\n\n      {/* Main Content */}\n      <AppShellMain>\n        <Grid gutter=\"md\">\n          {/* Top Row: Turbine Banner + Weather Cards */}\n          <GridCol span={7}>\n            <Paper\n              radius=\"lg\"\n              p=\"xl\"\n              style={{\n                height: '100%',\n                background: 'linear-gradient(135deg, #dccafc 0%, #f6dadd 100%)',\n                position: 'relative',\n                overflow: 'hidden',\n                color: '#fff',\n                display: 'flex',\n                flexDirection: 'column',\n                justifyContent: 'space-between',\n              }}\n            >\n              <Box style={{ zIndex: 1 }}>\n                <Text c=\"white\" style={{ opacity: 0.8 }} mb=\"xs\">\n                  Renewable Energy\n                </Text>\n                <Title order={1} style={{ lineHeight: 1.2, marginBottom: '1rem' }}>\n                  Vertical Axis Wind<br />Turbine (VAWT)\n                </Title>\n                <Group mb=\"lg\">\n                  <Badge color=\"white\" variant=\"light\" size=\"lg\" radius=\"sm\">\n                    Darrieus design\n                  </Badge>\n                  <Badge color=\"white\" variant=\"light\" size=\"lg\" radius=\"sm\">\n                    Savonius design\n                  </Badge>\n                </Group>\n                <Text c=\"white\" style={{ opacity: 0.9, maxWidth: '400px' }} mb=\"xl\">\n                  Air turbines, or more commonly known as wind turbines, are used to convert\n                  kinetic energy from the wind into mechanical energy, which can then be converted\n                  into electricity.\n                </Text>\n\n                <SimpleGrid cols={2} style={{ maxWidth: '400px' }} spacing=\"md\">\n                  <Group gap=\"xs\">\n                    <Text>üçÉ</Text>\n                    <Text size=\"sm\">3-25 m/s</Text>\n                  </Group>\n                  <Group gap=\"xs\">\n                    <Text>üîÑ</Text>\n                    <Text size=\"sm\">30-40% meter</Text>\n                  </Group>\n                  <Group gap=\"xs\">\n                    <Text>‚öôÔ∏è</Text>\n                    <Text size=\"sm\">10-200 RPM</Text>\n                  </Group>\n                  <Group gap=\"xs\">\n                    <Text>‚ö°</Text>\n                    <Text size=\"sm\">30-40% Efficiency</Text>\n                  </Group>\n                </SimpleGrid>\n              </Box>\n\n              {/* Decorative Turbine Image */}\n              <Image\n                src=\"https://picsum.photos/id/1056/400/600\"\n                alt=\"Wind Turbine\"\n                style={{\n                  position: 'absolute',\n                  right: '-50px',\n                  bottom: '-50px',\n                  height: '120%',\n                  width: 'auto',\n                  opacity: 0.9,\n                  mixBlendMode: 'overlay',\n                }}\n              />\n            </Paper>\n          </GridCol>\n\n          <GridCol span={5}>\n            <SimpleGrid cols={2} spacing=\"md\">\n              {weatherData.map((weather, index) => (\n                <Paper key={index} p=\"md\" radius=\"lg\" bg=\"white\" shadow=\"xs\">\n                  <Flex justify=\"space-between\" align=\"flex-start\" mb=\"sm\">\n                    <Box />\n                    <Text size=\"xl\">{weather.icon}</Text>\n                  </Flex>\n                  <Text fw={700} mb=\"xs\">\n                    {weather.title}\n                  </Text>\n                  <Text size=\"xs\" c=\"dimmed\" mb=\"lg\" style={{ minHeight: '40px' }}>\n                    {weather.desc}\n                  </Text>\n                  <Flex justify=\"space-between\" align=\"center\">\n                    <Box>\n                      <Text size=\"xs\" fw={700}>\n                        {weather.rotation}\n                      </Text>\n                      <Text size=\"xs\" c=\"dimmed\">\n                        Turbine Rotation\n                      </Text>\n                    </Box>\n                    <Box>\n                      <Text size=\"xs\" fw={700}>\n                        {weather.wind}\n                      </Text>\n                      <Text size=\"xs\" c=\"dimmed\">\n                        Wind Speed\n                      </Text>\n                    </Box>\n                  </Flex>\n                </Paper>\n              ))}\n            </SimpleGrid>\n          </GridCol>\n\n          {/* Bottom Row: Table + Company Details */}\n          <GridCol span={8}>\n            <Paper p=\"lg\" radius=\"lg\" bg=\"white\" shadow=\"xs\">\n              <Group justify=\"space-between\" mb=\"lg\">\n                <Title order={4}>State of Renewable Energy</Title>\n                <Group bg=\"gray.1\" p={4} styl