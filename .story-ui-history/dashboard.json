      "code": "import React from 'react';\nimport type { Meta, StoryObj } from '@storybook/react';\nimport { \n  Box, \n  SimpleGrid, \n  Card, \n  Title, \n  Text, \n  Badge, \n  Group, \n  Stack, \n  Table,\n  TableThead,\n  TableTbody,\n  TableTr,\n  TableTh,\n  TableTd,\n  Avatar,\n  AvatarGroup,\n  Progress,\n  Button,\n  ActionIcon,\n  Flex\n} from '@mantine/core';\n\nconst meta: Meta = {\n  title: 'Generated/Dashboard',\n  id: 'dashboard-ea878c38',\n  parameters: { layout: 'padded' },\n  tags: ['autodocs'],\n};\n\nexport default meta;\ntype Story = StoryObj;\n\nexport const Default: Story = {\n  render: () => (\n    <Box style={{ padding: '24px', backgroundColor: '#f5f5f5', minHeight: '100vh' }}>\n      {/* Header */}\n      <Flex justify=\"space-between\" align=\"center\" mb=\"xl\">\n        <Title order={2}>Overview</Title>\n        <Group>\n          <ActionIcon variant=\"subtle\" size=\"lg\">\n            <Text>üîç</Text>\n          </ActionIcon>\n          <ActionIcon variant=\"subtle\" size=\"lg\">\n            <Text>üîî</Text>\n          </ActionIcon>\n          <Button>+ Add a new note</Button>\n          <Avatar src=\"https://picsum.photos/40/40\" radius=\"xl\" />\n        </Group>\n      </Flex>\n\n      <SimpleGrid cols={2} spacing=\"lg\" mb=\"lg\">\n        {/* Left Column */}\n        <div>\n          {/* Hero Card - Wind Turbine */}\n          <Card \n            padding=\"xl\" \n            radius=\"lg\" \n            mb=\"lg\"\n            style={{ \n              background: 'linear-gradient(135deg, #d4b3e8 0%, #e8c5d4 50%, #f5d4c8 100%)',\n              minHeight: '400px',\n              position: 'relative'\n            }}\n          >\n            <Badge color=\"gray\" variant=\"light\" mb=\"md\">Renewable Energy</Badge>\n            <Title order={3} mb=\"xs\" style={{ color: 'white' }}>\n              Vertical Axis Wind Turbine (VAWT)\n            </Title>\n            <Group gap=\"xs\" mb=\"lg\">\n              <Badge variant=\"light\" color=\"gray\">bernard design</Badge>\n              <Badge variant=\"light\" color=\"gray\">savonius design</Badge>\n            </Group>\n            <Text size=\"sm\" mb=\"xl\" style={{ color: 'rgba(255,255,255,0.9)', maxWidth: '280px' }}>\n              Air turbines or more commonly known as wind turbines are used to convert kinetic energy from the wind into mechanical energy, which can then be converted into electricity.\n            </Text>\n            \n            <Stack gap=\"sm\" mb=\"md\">\n              <Group>\n                <Text size=\"sm\" style={{ color: 'rgba(255,255,255,0.8)' }}>‚ö° 3-25 m/s</Text>\n                <Text size=\"sm\" style={{ color: 'rgba(255,255,255,0.8)' }}>üå°Ô∏è 30-40% meter</Text>\n              </Group>\n              <Group>\n                <Text size=\"sm\" style={{ color: 'rgba(255,255,255,0.8)' }}>üîÑ 10-200 RPM</Text>\n                <Text size=\"sm\" style={{ color: 'rgba(255,255,255,0.8)' }}>‚ö° 30-40% Efficiency</Text>\n              </Group>\n            </Stack>\n\n            {/* Wind turbine illustration */}\n            <Box style={{ position: 'absolute', right: '20px', top: '50%', transform: 'translateY(-50%)' }}>\n              <Text style={{ fontSize: '200px', opacity: 0.3 }}>üè≠</Text>\n            </Box>\n\n            <Text size=\"xs\" style={{ color: 'rgba(255,255,255,0.7)' }}>Turbine ‚ñº</Text>\n          </Card>\n\n          {/* State of Renewable Energy Table */}\n          <Card padding=\"lg\" radius=\"lg\">\n            <Flex justify=\"space-between\" align=\"center\" mb=\"md\">\n              <Title order={4}>State of Renewable Energy</Title>\n              <Group gap=\"xs\">\n                <Badge variant=\"light\" color=\"gray\">All</Badge>\n                <Badge variant=\"light\" color=\"gray\">In-work</Badge>\n                <Badge variant=\"light\" color=\"gray\">Scheduled</Badge>\n              </Group>\n            </Flex>\n\n            <Table>\n              <TableThead>\n                <TableTr>\n                  <TableTh>Name</TableTh>\n                  <TableTh>Power</TableTh>\n                  <TableTh>Rotation</TableTh>\n                  <TableTh>Wind</TableTh>\n                  <TableTh>Status</TableTh>\n                </TableTr>\n              </TableThead>\n              <TableTbody>\n                <TableTr>\n                  <TableTd>Three-bladed HAWT</TableTd>\n                  <TableTd>‚ö° 1 kW - 10 Mw</TableTd>\n                  <TableTd>üîÑ 10-20 RPM</TableTd>\n                  <TableTd>üå™Ô∏è 3-25 m/s</TableTd>\n                  <TableTd><Badge color=\"green\" variant=\"light\">Active</Badge></TableTd>\n                </TableTr>\n                <TableTr>\n                  <TableTd>Multi-Blade HAWT</TableTd>\n                  <TableTd>‚ö° 500 W - 100 kW</TableTd>\n                  <TableTd>üîÑ 30-60 RPM</TableTd>\n                  <TableTd>üå™Ô∏è 3-25 m/s</TableTd>\n                  <TableTd><Badge color=\"green\" variant=\"light\">Active</Badge></TableTd>\n                </TableTr>\n                <TableTr>\n                  <TableTd>Darrieus VAWT</TableTd>\n                  <TableTd>‚ö° 1 kW - several MW</TableTd>\n                  <TableTd>üîÑ 10-200 RPM</TableTd>\n                  <TableTd>üå™Ô∏è 3-25 m/s</TableTd>\n                  <TableTd><Badge color=\"yellow\" variant=\"light\">Pending</Badge></TableTd>\n                </TableTr>\n                <TableTr>\n                  <TableTd>Savonius VAWT</TableTd>\n                  <TableTd>‚ö° 1 - 50 kW</TableTd>\n                  <TableTd>üîÑ 10-50 RPM</TableTd>\n                  <TableTd>üå™Ô∏è 2-16 m/s</TableTd>\n                  <TableTd><Badge color=\"red\" variant=\"light\">Non-Active</Badge></TableTd>\n                </TableTr>\n                <TableTr>\n                  <TableTd>Helical VAWT</TableTd>\n                  <TableTd>‚ö° 1 - 10 kW</TableTd>\n                  <TableTd>üîÑ 10-150 RPM</TableTd>\n                  <TableTd>üå™Ô∏è 3-20 m/s</TableTd>\n                  <TableTd><Badge color=\"green\" variant=\"light\">Active</Badge></TableTd>\n                </TableTr>\n              </TableTbody>\n            </Table>\n          </Card>\n        </div>\n\n        {/* Right Column */}\n        <div>\n          <SimpleGrid cols={2} spacing=\"md\" mb=\"lg\">\n            {/* Calm Weather Card */}\n            <Card padding=\"lg\" radius=\"lg\" style={{ backgroundColor: 'white' }}>\n              <Group mb=\"md\">\n                <Text style={{ fontSize: '48px' }}>‚òÄÔ∏è</Text>\n                <Box>\n                  <Title order={4}>Calm Weather</Title>\n                  <Text size=\"sm\" c=\"dimmed\">\n                    The turbine may not spin at all or only spin very slowly\n                  </Text>\n                </Box>\n              </Group>\n              <Group>\n                <Box>\n                  <Text fw={700} size=\"lg\">0-5</Text>\n                  <Text size=\"xs\" c=\"dimmed\">RPM</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Turbine Rotation</Text>\n                </Box>\n                <Box>\n                  <Text fw={700} size=\"lg\">0-3</Text>\n                  <Text size=\"xs\" c=\"dimmed\">m/s</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Wind Speed</Text>\n                </Box>\n              </Group>\n            </Card>\n\n            {/* Rainy Weather Card */}\n            <Card padding=\"lg\" radius=\"lg\" style={{ backgroundColor: 'white' }}>\n              <Group mb=\"md\">\n                <Text style={{ fontSize: '48px' }}>üåßÔ∏è</Text>\n                <Box>\n                  <Title order={4}>Rainy Weather</Title>\n                  <Text size=\"sm\" c=\"dimmed\">\n                    The rotating speed control system does not exceed the safe limit\n                  </Text>\n                </Box>\n              </Group>\n              <Group>\n                <Box>\n                  <Text fw={700} size=\"lg\">20-25</Text>\n                  <Text size=\"xs\" c=\"dimmed\">RPM</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Turbine Rotation</Text>\n                </Box>\n                <Box>\n                  <Text fw={700} size=\"lg\">12-15</Text>\n                  <Text size=\"xs\" c=\"dimmed\">m/s</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Wind Speed</Text>\n                </Box>\n              </Group>\n            </Card>\n\n            {/* Windy Weather Card */}\n            <Card padding=\"lg\" radius=\"lg\" style={{ backgroundColor: 'white' }}>\n              <Group mb=\"md\">\n                <Text style={{ fontSize: '48px' }}>üå§Ô∏è</Text>\n                <Box>\n                  <Title order={4}>Windy Weather</Title>\n                  <Text size=\"sm\" c=\"dimmed\">\n                    The rotating turbine varies proportionally with the wind speed\n                  </Text>\n                </Box>\n              </Group>\n              <Group>\n                <Box>\n                  <Text fw={700} size=\"lg\">10-20</Text>\n                  <Text size=\"xs\" c=\"dimmed\">RPM</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Turbine Rotation</Text>\n                </Box>\n                <Box>\n                  <Text fw={700} size=\"lg\">4-12</Text>\n                  <Text size=\"xs\" c=\"dimmed\">m/s</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Wind Speed</Text>\n                </Box>\n              </Group>\n            </Card>\n\n            {/* Stormy Weather Card */}\n            <Card padding=\"lg\" radius=\"lg\" style={{ backgroundColor: 'white' }}>\n              <Group mb=\"md\">\n                <Text style={{ fontSize: '48px' }}>‚ö°</Text>\n                <Box>\n                  <Title order={4}>Stormy Weather</Title>\n                  <Text size=\"sm\" c=\"dimmed\">\n                    The turbine stops spinning or uses the brake to stop the operation\n                  </Text>\n                </Box>\n              </Group>\n              <Group>\n                <Box>\n                  <Text fw={700} size=\"lg\">0</Text>\n                  <Text size=\"xs\" c=\"dimmed\">RPM</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Turbine Rotation</Text>\n                </Box>\n                <Box>\n                  <Text fw={700} size=\"lg\">15-25+</Text>\n                  <Text size=\"xs\" c=\"dimmed\">m/s</Text>\n                  <Text size=\"xs\" c=\"dimmed\">Wind Speed</Text>\n                </Box>\n              </Group>\n            </Card>\n          </SimpleGrid>\n\n          {/* Company Details Card */}\n          <Card padding=\"lg\" radius=\"lg\" style={{ backgroundColor: 'white' }}>\n            <Flex justify=\"space-between\" align=\"center\" mb=\"md\">\n              <Title order={4}>Company Details</Title>\n              <Group gap=\"xs\">\n                <Badge variant=\"light\" color=\"blue\">Company</Badge>\n                <Badge variant=\"light\" color=\"orange\">Turbines</Badge>\n              </Group>\n            </Flex>\n\n            <Text size=\"sm\" c=\"dimmed\" mb=\"md\">\n              Companies engaged in turbines usually manufacture and provide various types of turbines for industrial use, power generation, and other applications that require energy conversion.\n            </Text>\n\n            <Group mb=\"md\">\n              <Text size=\"sm\" fw={500}>Members</Text>\n              <AvatarGroup>\n                <Avatar src=\"https://picsum.photos/32/32?random=1\" radius=\"xl\" size=\"sm\" />\n                <Avatar src=\"https://picsum.photos/32/32?random=2\" radius=\"xl\" size=\"sm\" />\n                <Avatar src=\"https://picsum.photos/32/32?random=3\" radius=\"xl\" size=\"sm\" />\n                <Avatar src=\"https://picsum.photos/32/32?random=4\" radius=\"xl\" size=\"sm\" />\n              </AvatarGroup>\n            </Group>\n\n            <Box>\n              <Group justify=\"space-between\" mb=\"xs\">\n                <Text size=\"sm\" fw={500}>Company Performance</Text>\n                <Text size=\"sm\" fw={700}>100%</Text>\n              </Group>\n              <Progress value={75} color=\"blue\" size=\"md\" radius=\"xl\" />\n              <Text size=\"xs\" c=\"dimmed\" mt=\"xs\">75% Progress</Text>\n            </Box>\n          </Card>\n        </div>\n      </SimpleGrid>\n    </Box>\n  ),\n};",
