      "code": "import React from 'react';\nimport type { Meta, StoryObj } from '@storybook/react';\nimport { SimpleGrid, Card, CardSection, Image, Text, Badge, Button, Group } from '@mantine/core';\n\nconst meta: Meta = {\n  title: 'Generated/80s Music Card Grid',\n  id: '80s-music-card-grid-dfcd02dd',\n  parameters: { layout: 'padded' },\n  tags: ['autodocs'],\n};\n\nexport default meta;\ntype Story = StoryObj;\n\nexport const Default: Story = {\n  render: () => (\n    <SimpleGrid cols={4} spacing=\"lg\">\n      <Card shadow=\"sm\" padding=\"lg\" radius=\"md\" withBorder>\n        <CardSection>\n          <Image\n            src=\"https://picsum.photos/300/200?random=1\"\n            height={160}\n            alt=\"Michael Jackson Thriller Album\"\n          />\n        </CardSection>\n        <Group justify=\"space-between\" mt=\"md\" mb=\"xs\">\n          <Text fw={500}>Thriller</Text>\n          <Badge color=\"pink\">1982</Badge>\n        </Group>\n        <Text size=\"sm\" c=\"dimmed\">\n          Michael Jackson's iconic album featuring hits like \"Beat It\" and \"Billie Jean\". The best-selling album of all time.\n        </Text>\n        <Button color=\"violet\" fullWidth mt=\"md\" radius=\"md\">\n          Listen Now\n        </Button>\n      </Card>\n\n      <Card shadow=\"sm\" padding=\"lg\" radius=\"md\" withBorder>\n        <CardSection>\n          <Image\n            src=\"https://picsum.photos/300/200?random=2\"\n            height={160}\n            alt=\"Prince Purple Rain Album\"\n          />\n        </CardSection>\n        <Group justify=\"space-between\" mt=\"md\" mb=\"xs\">\n          <Text fw={500}>Purple Rain</Text>\n          <Badge color=\"grape\">1984</Badge>\n        </Group>\n        <Text size=\"sm\" c=\"dimmed\">\n          Prince's masterpiece soundtrack album. Features legendary tracks like \"When Doves Cry\" and the title song.\n        </Text>\n        <Button color=\"violet\" fullWidth mt=\"md\" radius=\"md\">\n          Listen Now\n        </Button>\n      </Card>\n\n      <Card shadow=\"sm\" padding=\"lg\" radius=\"md\" withBorder>\n        <CardSection>\n          <Image\n            src=\"https://picsum.photos/300/200?random=3\"\n            height={160}\n            alt=\"A-ha Take On Me Single\"\n          />\n        </CardSection>\n        <Group justify=\"space-between\" mt=\"md\" mb=\"xs\">\n          <Text fw={500}>Take On Me</Text>\n          <Badge color=\"cyan\">1985</Badge>\n        </Group>\n        <Text size=\"sm\" c=\"dimmed\">\n          A-ha's synth-pop classic with the revolutionary pencil-sketch music video. Defined the sound of the 80s.\n        </Text>\n        <Button color=\"violet\" fullWidth mt=\"md\" radius=\"md\">\n          Listen Now\n        </Button>\n      </Card>\n\n      <Card shadow=\"sm\" padding=\"lg\" radius=\"md\" withBorder>\n        <CardSection>\n          <Image\n            src=\"https://picsum.photos/300/200?random=4\"\n            height={160}\n            alt=\"Bon Jovi Slippery When Wet Album\"\n          />\n        </CardSection>\n        <Group justify=\"space-between\" mt=\"md\" mb=\"xs\">\n          <Text fw={500}>Slippery When Wet</Text>\n          <Badge color=\"red\">1986</Badge>\n        </Group>\n        <Text size=\"sm\" c=\"dimmed\">\n          Bon Jovi's breakthrough album featuring arena rock anthems \"Livin' on a Prayer\" and \"You Give Love a Bad Name\".\n        </Text>\n        <Button color=\"violet\" fullWidth mt=\"md\" radius=\"md\">\n          Listen Now\n        </Button>\n      </Card>\n    </SimpleGrid>\n  ),\n};\n\nexport const Compact: Story = {\n  render: () => (\n    <SimpleGrid cols={2} spacing=\"md\">\n      <Card shadow=\"xs\" padding=\"md\" radius=\"sm\" withBorder>\n        <Group>\n          <Image\n            src=\"https://picsum.photos/80/80?random=5\"\n            width={80}\n            height={80}\n            radius=\"sm\"\n            alt=\"Depeche Mode\"\n          />\n          <div>\n          